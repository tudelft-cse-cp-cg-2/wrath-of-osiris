<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.context.cg2.client.controller.MainMenuController">
 <center>
    <GridPane BorderPane.alignment="CENTER">
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
       <children>
          <Button fx:id="joinGameButton" mnemonicParsing="false" prefHeight="57.0" prefWidth="116.0" text="Join&#10;Game" textAlignment="CENTER" GridPane.halignment="CENTER" GridPane.rowIndex="1">
             <font>
                <Font name="Arial Bold" size="16.0" />
             </font>
          </Button>
          <Button fx:id="createGameButton" mnemonicParsing="false" prefHeight="57.0" prefWidth="116.0" text="Create&#10;Game" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1">
             <font>
                <Font name="Arial Bold" size="16.0" />
             </font></Button>
          <Button fx:id="quitButton" mnemonicParsing="false" onAction="#pressQuit" prefHeight="32.0" prefWidth="59.0" text="Quit" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="BOTTOM">
             <GridPane.margin>
                <Insets bottom="30.0" right="30.0" />
             </GridPane.margin>
             <font>
                <Font name="Arial Bold" size="13.0" />
             </font>
          </Button>
       </children>
    </GridPane>
 </center>
</BorderPane>
